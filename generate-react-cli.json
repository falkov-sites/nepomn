{
  "usesTypeScript": false,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "grc_templates/component/TemplateName.tsx",
        "style": "grc_templates/component/TemplateName.module.scss"
      },
      "path": "components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page": {
      "customTemplates": {
        "component": "grc_templates/page/page.tsx",
        "style": "grc_templates/page/TemplateName.module.scss",
        "layout": "grc_templates/page/layout.tsx",
        "loading": "grc_templates/page/loading.tsx",
        "error": "grc_templates/page/error.tsx"
      },
      "path": "app",
      "withStyle": true,
      "withLayout": true,
      "withLoading": true,
      "withError": true,

      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "withHook": false
    }
  }
}